{
    "C_Cpp.intelliSenseEngine": "disabled",
    "ceedlingExplorer.debugConfiguration": "Test Debug",
    "ceedlingExplorer.projectPath": "./c_tool_box",
    "clangd.arguments": [
        "--header-insertion=never",
        "--compile-commands-dir=c_tool_box/build/artifacts",
    ],
    "[c]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "C/C++ Include Guard.Macro Type": "Filename",
    "C/C++ Include Guard.Comment Style": "Line",
    "C/C++ Include Guard.Path Depth": 0,
    "C/C++ Include Guard.Path Skip": 0,
    "C/C++ Include Guard.Spaces After Endif": 1,
    "C/C++ Include Guard.Remove Extension": false,
}